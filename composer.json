{
    "name": "qlimix/dependency-container",
    "type": "library",
    "description": "Registering dependencies on a PSR container and setup interfaces",
    "license": "MIT",
    "keywords": [
        "container",
        "dependency",
        "di"
    ],
    "authors": [
        {
            "name": "Frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">=7.4",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "qlimix/code-standard": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\DependencyContainer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\DependencyContainer\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
